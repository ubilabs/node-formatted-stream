{
  "name": "formatted-stream",
  "version": "1.0.0",
  "description": "Streaming XLSX, CSV, and JSON parser and writer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "generatechangelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "lint": "eslint ./src ./test",
    "prepublish": "rm -rf dist/ && npm run build",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register",
    "release": "release-it"
  },
  "author": {
    "name": "Keno Schwalb",
    "email": "schwalb@ubilabs.net",
    "url": "https://www.ubilabs.net"
  },
  "keywords": [
    "csv",
    "json",
    "xlsx",
    "excel",
    "stream",
    "streaming",
    "parser",
    "writer"
  ],
  "repository": "ubilabs/node-formatted-stream",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.1.2",
    "exceljs": "0.2.38",
    "fast-csv": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.2.2",
    "mocha": "^3.0.0",
    "release-it": "^2.4.3"
  },
  "engines": {
    "node": ">=4"
  }
}
