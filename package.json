{
  "name": "formatted-stream",
  "version": "0.0.1",
  "description": "Streaming XLSX, CSV, and JSON parser and writer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "lint": "eslint ./src ./test",
    "prepublish": "npm run test && npm run build",
    "pretest": "npm run lint",
    "release": "release-it",
    "test": "mocha --compilers js:babel-register"
  },
  "author": {
    "name": "Keno Schwalb",
    "email": "schwalb@ubilabs.net",
    "url": "http://ubilabs.net"
  },
  "keywords": [
    "csv",
    "json",
    "xlsx",
    "excel",
    "stream",
    "streaming",
    "parser",
    "writer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubilabs/node-formatted-stream"
  },
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.1.2",
    "exceljs": "0.2.11",
    "fast-csv": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^3.2.2",
    "mocha": "^3.0.0",
    "release-it": "^2.4.3"
  },
  "engines": {
    "node": ">=4"
  }
}
